{
  "builds": [
    {
      "src": "Projects/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "Projects/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "Projects/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Projects/wsgi.py"
    }
  ]
}